@model IEnumerable<NHibernate.AspNet.Identity.IdentityRole>

@{
    ViewBag.Title = "Role List";
}

<hr/>

<table class="table table-bordered table-hover">
    <tr>
        <th>Role Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
        </tr>
    }

</table>

<hr/>

@Html.ActionLink("Create new role", "CreateRole", "Role") 
<hr/>
@Html.ActionLink("Admin page", "Index", "Admin")

