@using CargoLogistic.Domain.Entities
@model IEnumerable<Country>

@{
    ViewBag.Title = "List of Country";
}

@foreach (var c in Model)
{
    <div>
        <h3>@c.Alpha2Code - @c.Name - @c.NumericCode</h3>
        <p>=============================</p>
    </div>
}

@Html.ActionLink("Home", "Index", "Home")

